{
  "name": "main",
  "expo": {
    "name": "Boat Rental App",
    "slug": "boat-rental-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.boatrentals.app",
      "supportsTablet": true
    },
    "android": {
      "package": "com.boatrentals.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUseUsageDescription": "Allow Boat Rental App to use your location."
        }
      ]
    ],
    "jsEngine": "hermes"
  }
}
